    "import requests\n",
    "url = 'https://www.technologyreview.com/'\n",
    "res = requests.get(url)\n",
    "from bs4 import BeautifulSoup\n",
    "soup = BeautifulSoup(res.text)\n",
    "h2 = soup.select('.hp-daily__tz__title')\n",
    "for h in h2:\n",
    "    print h.text"
   ]
